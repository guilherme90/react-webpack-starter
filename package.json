{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "Simple project to stater React project with Webpack hot middleware, react-hot-loader and more",
  "main": "./src/index.js",
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development PORT=8080 API_URL=http://localhost.dev node ./server.js --exec babel-node",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress --profile --colors",
    "prod-server": "cross-env NODE_ENV=production PORT=8080 API_URL=http://localhost.prod node ./server.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guilherme90/react-webpack-starter.git"
  },
  "keywords": [
    "react",
    "webpack",
    "bootstrap"
  ],
  "author": "Guilherme P. Nogueira <guilhermenogueira90@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guilherme90/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/guilherme90/react-webpack-starter#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "opn": "^4.0.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.3",
    "reactstrap": "^4.4.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
